{
  "name": "@vzh/mobx-stores",
  "version": "1.15.0",
  "description": "MobX stores",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "Vladimir Zhukov",
  "license": "MIT",
  "contributors": [
    {
      "name": "Vladimir Zhukov",
      "url": "http://github.com/vlazh"
    }
  ],
  "scripts": {
    "update-deps": "yarn upgrade-interactive --latest",
    "clean": "rimraf dist/*",
    "copy:configs": "copyfiles package.json README.md LICENSE ./dist/",
    "build": "npm run clean && tsc --project ./tsconfig.json && npm run copy:configs",
    "build-to-tutoron": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/tutoron/node_modules/@vzh/mobx-stores/' && copyfiles package.json '/home/vladimir/dev/sources/tutoron/node_modules/@vzh/mobx-stores/'",
    "build-to-lcardio-app": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/lcardio-app/node_modules/@vzh/mobx-stores/' && copyfiles package.json '/home/vladimir/dev/sources/lcardio-app/node_modules/@vzh/mobx-stores/'",
    "build-to-react-components": "tsc --project ./tsconfig.json --outDir '../react-components/node_modules/@vzh/mobx-stores/' && copyfiles package.json '../react-components/node_modules/@vzh/mobx-stores/'",
    "patch-publish": "npm version patch && npm run build && npm publish ./dist/ && git push --follow-tags",
    "minor-publish": "npm version minor && npm run build && npm publish ./dist/ && git push --follow-tags",
    "major-publish": "npm version major && npm run build && npm publish ./dist/ && git push --follow-tags"
  },
  "devDependencies": {
    "@types/jwt-decode": "^2.2.1",
    "@types/react": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "@vzh/configs": "^3.39.0",
    "@vzh/ts-types": "^0.8.1",
    "copyfiles": "^2.1.1",
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "jwt-decode": "^2.2.0",
    "localforage": "^1.7.3",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "mobx-utils": "^5.4.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.3",
    "valtors": "^2.1.0"
  },
  "peerDependencies": {
    "@vzh/ts-types": "*"
  },
  "prettier": "@vzh/configs/prettier"
}
