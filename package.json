{
  "name": "@vzh/mobx-utils",
  "version": "1.28.1",
  "description": "MobX utils",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "Vladimir Zhukov",
  "license": "MIT",
  "contributors": [
    {
      "name": "Vladimir Zhukov",
      "url": "http://github.com/vlazh"
    }
  ],
  "homepage": "https://github.com/vlazh/mobx-utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/vlazh/mobx-utils"
  },
  "bugs": {
    "url": "https://github.com/vlazh/mobx-utils/issues"
  },
  "keywords": [
    "mobx",
    "mobx utils",
    "mobx store",
    "mobx model",
    "mobx serialization"
  ],
  "scripts": {
    "update-deps": "yarn upgrade-interactive --latest",
    "clean": "rimraf dist/*",
    "copy:configs": "copyfiles package.json README.md LICENSE ./dist/",
    "build": "npm run clean && tsc --project ./tsconfig.json && npm run copy:configs",
    "build-to-tutoron": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/tutoron/node_modules/@vzh/mobx-utils/' && copyfiles package.json '/home/vladimir/dev/sources/tutoron/node_modules/@vzh/mobx-utils/'",
    "build-to-react-components": "tsc --project ./tsconfig.json --outDir '../react-components/node_modules/@vzh/mobx-utils/' && copyfiles package.json '../react-components/node_modules/@vzh/mobx-utils/'",
    "patch-publish": "npm version patch && npm run build && npm publish ./dist/ --access public && git push --follow-tags",
    "minor-publish": "npm version minor && npm run build && npm publish ./dist/ --access public && git push --follow-tags",
    "major-publish": "npm version major && npm run build && npm publish ./dist/ --access public && git push --follow-tags"
  },
  "devDependencies": {
    "@types/jwt-decode": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "@vzh/configs": "^3.43.1",
    "@vzh/ts-types": "^1.2.0",
    "copyfiles": "^2.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jwt-decode": "^2.2.0",
    "localforage": "^1.7.3",
    "mobx": "^5.15.2",
    "mobx-utils": "^5.5.3",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.4",
    "valtors": "^2.2.1"
  },
  "prettier": "@vzh/configs/prettier"
}
