{
  "name": "@vzh/mobx-utils",
  "version": "2.4.0",
  "description": "MobX utils",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "Vladimir Zhukov",
  "license": "MIT",
  "keywords": [
    "mobx",
    "mobx utils",
    "mobx store",
    "mobx model",
    "mobx serialization"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "copy:configs": "copyfiles package.json README.md LICENSE .npmignore ./dist/",
    "build": "npm run clean && tsc --project ./tsconfig.json",
    "build-to-tutoron": "tsc --project ./tsconfig.json --outDir '/home/vladimir/dev/sources/tutoron/node_modules/@vzh/mobx-utils/' && copyfiles package.json '/home/vladimir/dev/sources/tutoron/node_modules/@vzh/mobx-utils/'",
    "build-to-react-components": "tsc --project ./tsconfig.json --outDir '../react-components/node_modules/@vzh/mobx-utils/' && copyfiles package.json '../react-components/node_modules/@vzh/mobx-utils/'",
    "patch-publish": "npm run build && npm version patch && npm run copy:configs && npm publish ./dist/ --access public && git push --follow-tags",
    "minor-publish": "npm run build && npm version minor && npm run copy:configs && npm publish ./dist/ --access public && git push --follow-tags",
    "major-publish": "npm run build && npm version major && npm run copy:configs && npm publish ./dist/ --access public && git push --follow-tags"
  },
  "devDependencies": {
    "@types/jwt-decode": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "@vzh/configs": "^3.56.0",
    "@vzh/ts-types": "^1.6.0",
    "copyfiles": "^2.3.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jwt-decode": "^2.2.0",
    "localforage": "^1.7.3",
    "mobx": "^5.15.4",
    "mobx-utils": "^5.6.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3",
    "valtors": "^2.3.0"
  },
  "prettier": "@vzh/configs/prettier"
}
